{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "outDir": "./build/ide/ts",
    "rootDir": "./src",
    "baseUrl": "./node_modules",
    "removeComments": false,
    "stripInternal": true,
    "preserveConstEnums": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noImplicitReturns": false,
    "newLine": "LF",
    "noEmitOnError": true,
    "moduleResolution": "node",
    "keyofStringsOnly": true,
    "preserveSymlinks": true,
    "paths": {
      "utils/*": ["utils/*"],
      "mock/*": ["mock/*"],
      "components/*": ["components/*"],
      "services/*": ["services/*"]
    }
  },
  "include": [
    "./src/code/**/*.ts",
    "./src/code/**/*.tsx",
    "./node_modules/*/module.d.ts",
    "./node_modules/*/module-protected.d.ts",
    "./src/tests/jest/**/*.ts", "src/code/components/abc-seatmap/widgets/SeatMapShoppingTile.ts", "src/code/components/abc-seatmap/widgets/SeatMapShoppingView.ts",
  ],
  "exclude": [
    "./node_modules/sabre-ngv-skeleton/**/*"
  ]
}